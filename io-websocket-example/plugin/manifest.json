{
    "id": "com.adobe.example.uxp-websocket",
    "name": "Websocket Example",
    "version": "1.0.0",
    "main": "index.html",
    "host": [{
        "app": "PS",
        "minVersion": "22.0.0"
    }],
    "manifestVersion": 4,
    "entrypoints": [
        {
            "type": "panel",
            "id": "websocket",
            "minimumSize": {"width": 230, "height": 200},
            "maximumSize": {"width": 2000, "height": 2000},
            "preferredDockedSize": {"width": 230, "height": 300},
            "preferredFloatingSize": {"width": 230, "height": 300},
            "label": {"default": "Websocket Example"}
        }
    ],
    "icons": [
        {
            "width": 23, "height": 23, "path": "icons/dark.png", "scale": [ 1, 2 ],
            "theme": [ "darkest", "dark", "medium" ]
        }, {
            "width": 23, "height": 23, "path": "icons/light.png", "scale": [ 1, 2 ],
            "theme": [ "lightest", "light" ]
        }
    ]
}
