{
    "id": "com.adobe.webservice-js-sample",
    "name": "Sample: Webservice Query",
    "version": "1.0.0",
    "main": "index.html",
    "host": [{
        "app": "PS",
        "minVersion": "23.0.0"
    }],
    "manifestVersion": 4,
    "entrypoints": [
        {
            "type": "panel",
            "id": "webservice-query",
            "minimumSize": {"width": 230, "height": 200},
            "maximumSize": {"width": 1000, "height": 1000},
            "preferredDockedSize": {"width": 230, "height": 200},
            "preferredFloatingSize": {"width": 300, "height": 300},
            "label": {"default": "Webservice Query"},
      "icons": [
        {
          "width": 23,
          "height": 23,
          "path": "icons/dark.png",
          "scale": [
            1,
            2
          ],
          "theme": [
            "darkest",
            "dark",
            "medium"
          ]
        },
        {
          "width": 23,
          "height": 23,
          "path": "icons/light.png",
          "scale": [
            1,
            2
          ],
          "theme": [
            "lightest",
            "light"
          ]
        }
      ]
    }
    ],
	"icons": [
    {
      "width": 48,
      "height": 48,
      "path": "icons/plugin-icon.png",
      "scale": [
        1,
        2
      ],
      "theme": [
        "darkest",
        "dark",
        "medium",
        "lightest",
        "light",
        "all"
      ],
      "species": [
        "pluginList"
      ]
    }
  ]
}
