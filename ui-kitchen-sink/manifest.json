{
    "id": "com.adobe.example.uxp-ui-kitchen-sink",
    "name": "UXP UI Kitchen Sink",
    "version": "2.0.0",
    "main": "index.html",
    "host": {
        "app": "PS",
        "minVersion": "23.3.0",
        "data": {
            "apiVersion": 2
        }
    },
    "manifestVersion": 5,
    "requiredPermissions": {
        "clipboard": "readAndWrite",
        "localFileSystem": "request",
        "launchProcess": {
            "schemes": ["https", "mailto"],
            "extensions": [".png", ""]
        },
        "network": {
            "domains": [ "https://images.unsplash.com" ]
        },
        "webview": {
            "allow": "yes",
            "domains": [ "https://*.adobe.com", "https://*.example.com" ]
        }
    },
    "entrypoints": [
        {
            "type": "command",
            "id": "reloadPlugin",
            "label": "Reload Plugin"
        },
        {
            "type": "panel",
            "id": "kitchenSink",
            "minimumSize": {"width": 230, "height": 200},
            "maximumSize": {"width": 2000, "height": 2000},
            "preferredDockedSize": {"width": 320, "height": 512},
            "preferredFloatingSize": {"width": 512, "height": 512},
            "label": {"default": "Kitchen Sink"},
            "icons": [
                {
                    "width": 23, "height": 23, "path": "icons/dark.png",
                    "scale": [ 1, 2 ],
                    "theme": [ "darkest", "dark", "medium" ]
                },
                {
                    "width": 23, "height": 23, "path": "icons/light.png",
                    "scale": [ 1, 2 ],
                    "theme": [ "lightest", "light" ]
                }
            ]
        },
        {
            "type": "panel",
            "id": "secondPanel",
            "minimumSize": {"width": 230, "height": 200},
            "maximumSize": {"width": 2000, "height": 2000},
            "preferredDockedSize": {"width": 230, "height": 300},
            "preferredFloatingSize": {"width": 230, "height": 300},
            "label": {"default": "Second Panel Demo"},
            "icons": [
                {
                    "width": 23, "height": 23, "path": "icons/dark.png",
                    "scale": [ 1, 2 ],
                    "theme": [ "darkest", "dark", "medium" ]
                },
                {
                    "width": 23, "height": 23, "path": "icons/light.png",
                    "scale": [ 1, 2 ],
                    "theme": [ "lightest", "light" ]
                }
            ]
        }
    ],
    "icons": [
        {
            "width": 48, "height": 48, "path": "icons/plugin.png", "scale": [ 1, 2 ],
            "theme": [ "darkest", "dark", "medium", "lightest", "light", "all" ],
            "species": [ "pluginList" ]
        }
    ]
}
